[apply]
	whitespace = fix
[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes
	whitespace = fix,space-before-tab,tab-in-indent,trailing-space
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	log = true
[url "git@bitbucket.org:"]
	insteadOf = "bb:"
	pushInsteadOf = "bitbucket:"
	pushInsteadOf = "git://bitbucket.org/"
[url "git@github.com:"]
	insteadOf = "gh:"
	pushInsteadOf = "github:"
	pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
	insteadOf = "github:"
[url "git@gist.github.com:"]
	insteadOf = "gst:"
	pushInsteadOf = "gist:"
	pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
	insteadOf = "gist:"
[alias]
  st = status
	ss = status --short
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
	lp = log --pretty=oneline
	graph = log --pretty=format:'%h : %s' --graph
	amend = commit --amend -C HEAD
